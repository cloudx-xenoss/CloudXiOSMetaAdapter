{
  "name": "CloudXMetaAdapter",
  "version": "1.0.3",
  "summary": "Mobile SDK for CloudX iOS Meta Adapter",
  "description": "iOS adapter add-on to the the CloucX iOS SDK for a Meta bidder",
  "homepage": "https://github.com/cloudx-xenoss/CloudXiOSMetaAdapter",
  "authors": {
    "CloudX": "support@cloudx.com"
  },
  "license": {
    "type": "Proprietary",
    "text": "Copyright 2024 CloudX, Inc. All rights reserved."
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "module_name": "CloudXMetaAdapter",
  "source": {
    "http": "https://github.com/cloudx-xenoss/CloudXiOSMetaAdapter/releases/download/1.0.3/CloudXMetaAdapter.xcframework.zip",
    "type": "zip",
    "flatten": false
  },
  "vendored_frameworks": "CloudXMetaAdapter.xcframework",
  "dependencies": {
    "CloudXCore": ">= 1.0.0",
    "FBAudienceNetwork": "~> 6.16"
  },
  "frameworks": [
    "AVFoundation",
    "AVKit",
    "AdSupport",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "Foundation",
    "StoreKit",
    "SystemConfiguration",
    "UIKit"
  ],
  "weak_frameworks": [
    "Combine",
    "CryptoKit",
    "SafariServices",
    "SwiftUI",
    "WebKit"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  }
}
